            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </TableView>
      <VBox prefHeight="400.0" prefWidth="115.0">
          <Button fx:id="dodajGradButton" mnemonicParsing="false" prefHeight="27.0" prefWidth="113.0" styleClass="confirm-button" text="Dodaj grad" onAction="#handleDodajGradButtonClick">
