            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding></Button>
          <Button fx:id="izmijeniGradButton" mnemonicParsing="false" prefHeight="27.0" prefWidth="108.0" styleClass="confirm-button" text="Izmijeni grad" onAction="#handleIzmijeniGradButtonClick">
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
