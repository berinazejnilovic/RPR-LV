            <TableColumn fx:id="stanovnikaColumn" prefWidth="150.0" text="Stanovnika" />
            <TableColumn fx:id="drzavaColumn" prefWidth="150.0" text="Država" />
        </columns>
         <HBox.margin>
            <Insets bottom="5.0" left="5.0" top="5.0" />
